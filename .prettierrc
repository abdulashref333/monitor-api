{
    "semi": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "printWidth": 90,
    "singleQuote": false,
    "useTabs": false,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "arrowParens": "avoid",
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "preserve",
    "endOfLine": "lf"
}
